{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "interface-name": [
      false
    ],
    "jsx-no-multiline-js": [
      false
    ]
  },
  "rulesDirectory": []
}